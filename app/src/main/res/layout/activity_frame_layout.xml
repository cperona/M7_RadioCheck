<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".frame_layout">

    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView"
            android:layout_marginTop="400dp"
            android:layout_marginLeft="170dp"
    />
    <ImageView
            android:layout_width="240dp"
            android:layout_height="193dp" tools:srcCompat="@tools:sample/avatars" android:id="@+id/image"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="50dp"
    />
    <Button
            android:text="Button"
            android:layout_width="131dp"
            android:layout_height="wrap_content" android:id="@+id/button2"
            android:layout_marginLeft="140dp"
            android:layout_marginTop="270dp"
    />
    <ImageButton
            android:layout_width="78dp"
            android:layout_height="wrap_content" app:srcCompat="?attr/actionModeCloseDrawable"
            android:id="@+id/imageButton2" android:onClick="fletxaEnreraOnClick"/>
</FrameLayout>